#MaximumSubArray
#code

class Solution:
    def maxSubArray(self, nums: List[int]) -> int:
        maximum_subarray=nums[0]
        current_sum=0

        for i in nums:
            if current_sum<0:
                current_sum=0
            current_sum+=i
            maximum_subarray=max(current_sum,maximum_subarray)
        return maximum_subarray

#TimeComplexity: O(N)

Input:-2 1 -3 4 -1 2 1 -5 4
output: 6

def maxSubArray(N):
    maxSub=N[0]
    curSum=0
    for i in N:
        if curSum<0:
            curSum=0
        curSum+=i
        maxSub=max(maxSub,curSum)
    return maxSub

N=list(map(int,input().split()))
print(maxSubArray(N))


